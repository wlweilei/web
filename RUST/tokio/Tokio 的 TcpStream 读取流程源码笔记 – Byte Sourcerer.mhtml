	// =E5=AD=98=E5=82=A8 readiness =E7=8A=B6=E6=80=81
    readiness: AtomicUsize,

    waiters: Mutex&lt;Waiters&gt;,
}

struct Waiters {=20
    /// List of all current waiters.
    list: WaitList,

    /// Waker used for `AsyncRead`.
    reader: Option&lt;Waker&gt;,

    /// Waker used for `AsyncWrite`.
    writer: Option&lt;Waker&gt;,
}
</code></pre>



<pre class=3D"wp-block-code"><code>impl ScheduleInfo {
    pub(super) fn poll_readiness(
